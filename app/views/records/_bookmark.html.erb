<div class="col-md-4 bookmark-list">
  
  <div class="panel panel-success">
    <div class="panel-heading">
      <h3 class="panel-title">BookMark List
        <button class="bookmark-add-btn pull-right">
          <span>
            <i class="glyphicon glyphicon-plus"></i>
          </span>
        </button>
        
      </h3>
      
    </div>
    <div class="list-group">
        <a href="#" class="list-group-item" data-bookmark="1">
          <span class="glyphicon bookmark-bullet" style="background-color: red;"></span>
          중요
        </a>
        <a href="#" class="list-group-item" data-bookmark="2">
          <span class="glyphicon bookmark-bullet" style="background-color: blue;"></span>
          안중요
        </a>
        <a href="#" class="list-group-item" data-bookmark="3">
          <span class="glyphicon bookmark-bullet" style="background-color: green;"></span>
          듣지마
        </a>
      </div>
  </div>
    <div class="panel panel-success">    
  </div>  
  <div class="panel panel-success">
    <div class="panel-heading">
      <h3 class="panel-title">Audio Controller</h3>
    </div>
    <div class="list-group">
        <div class="paly-controller list-group-item">
        <button class="controll-btn">
          <div>
            <span>
              <i class="glyphicon glyphicon-backward"></i>
            </span>
          </div>
          <div class="cotroll-btn-description">
            Previous
          </div>
        </button>
        <button class="controll-btn">
          <div>
            <div>
              <span id="play">
                <i class="glyphicon glyphicon-play"></i>
              </span>
            </div>
            <div class="cotroll-btn-description">
              Play
            </div>
          </div>

          <div style="display: none;">
            <div>
              <span id="play">
                <i class="glyphicon glyphicon-pause"></i>
              </span>
            </div>
            <div class="cotroll-btn-description">
              Pause
            </div>
          </div>
        </button>
        <button class="controll-btn">
          <div>
            <span>
              <i class="glyphicon glyphicon-forward"></i>
            </span>
          </div>
          <div class="cotroll-btn-description">
            Next
          </div>
        </button>
      </div>

      <div class="speed-controller list-group-item">
        <button id="speed-down-btn" class="controll-btn">
           <div>
              <span>
                <i class="glyphicon glyphicon-backward"></i>
              </span>
            </div>
            <div class="cotroll-btn-description">
              down
            </div>
        </button>
        <b><span id="speed-info">1.0</span>x</b>
        <button id="speed-up-btn" class="controll-btn">
          <div>
            <span>
              <i class="glyphicon glyphicon-forward"></i>
            </span>
          </div>
          <div class="cotroll-btn-description">
            up
          </div>
        </button>
      </div>
    </div>
      </div>
    <div class="panel-body">
      
  </div>

  
  </div>
</div>